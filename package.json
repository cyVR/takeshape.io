  "scripts": {
    "init": "tsg init",
    "clean": "rm -rf static/assets/javascripts && rm -rf static/assets/stylesheets && rm -rf static/assets/images && rm -f static/assets/*.json",
    "lint:js": "eslint src/javascripts",
    "lint:css": "stylelint src/stylesheets/**/*.scss --syntax scss",
    "lint": "npm-run-all -p lint:*",
    "imagemin": "imagemin src/images/* -o static/assets/images",
    "images:dev": "cp -R src/images static/assets/images",
    "icons": "svgo -f src/images/icons && mkdir -p static/assets/images && svg-sprite-generate -d src/images/icons -o static/assets/images/icons.svg && svgo static/assets/images/icons.svg",
    "icons:dev": "mkdir -p static/assets/images && svg-sprite-generate -d src/images/icons -o static/assets/images/icons.svg && svgo static/assets/images/icons.svg",
    "serve": "node -e 'setTimeout(function() {}, 2000)' && browser-sync start --https --server build --port 5000 --files 'build/**/*, !build/sitemap.xml, !build/feed.xml' --reload-debounce",
    "tsg:build": "tsg build",
    "build:images": "npm run imagemin && npm run icons",
    "build:images:dev": "npm run images:dev && npm run icons:dev",
    "build:webpack": "NODE_ENV=production webpack --display-error-details -p",
    "build:webpack:dev": "webpack --display-error-details -d",
    "build": "npm-run-all --serial clean build:images build:webpack",
    "deploy": "npm run build && tsg deploy",
    "watch:js": "webpack --watch",
    "watch:images": "chokidar 'src/images/**/*' -d -c 'npm run build:images'",
    "watch:tsg": "tsg watch",
    "watch": "npm-run-all --serial clean build:images:dev build:webpack:dev tsg:build && npm-run-all --continue-on-error --parallel watch:* serve",
    "start": "npm run watch"
  },
